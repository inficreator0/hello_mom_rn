{
  "expo": {
    "name": "Nova",
    "slug": "nova",
    "scheme": "nova",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./public/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./public/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./public/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "useNextNotificationsApi": true,
      "googleServicesFile": "./google-services.json",
      "package": "com.offclockengineers.nova",
      "versionCode": 1,
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "nova"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./public/favicon.ico"
    },
    "extra": {
      "eas": {
        "projectId": "22bc4fa5-8154-481f-af47-0ac04a2fd5de"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {},
    "plugins": [
      "expo-updates",
      "expo-notifications"
    ]
  }
}